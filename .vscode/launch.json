{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "deepstream",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build_debug/deepstream",
      "args": [
        "../../sample_720p.h264"
      ],
      "stopAtEntry": true,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "为 gdb 启用整齐打印",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}